{
  "name": "webapm_auth",
  "version": "1.0.0",
  "description": "WebAPM Authentication Server",
  "main": "app.js",
  "scripts": {
    "jsdoc": "node_modules/.bin/cross-env NODE_ENV=development node_modules/.bin/jsdoc -c jsdoc.json",
    "jsdoc:sdk": "node_modules/.bin/cross-env NODE_ENV=development node_modules/.bin/jsdoc -c jsdoc.sdk.json --readme ./client/src/sdk/sdk.md",
    "dev": "node_modules/.bin/cross-env NODE_ENV=development gulp --gulpfile gulp.dev.js",
    "build": "node_modules/.bin/cross-env NODE_ENV=production gulp --gulpfile gulp.prod.js",
    "start": "node_modules/.bin/cross-env NODE_ENV=production node build/app.js",
    "boot": "node_modules/.bin/cross-env NODE_ENV=production node build/app.js",
    "pm2:start": "node_modules/.bin/cross-env NODE_ENV=production pm2 start ahribori.auth.yml",
    "pm2:stop": "node_modules/.bin/pm2 stop AHRIBORI.OAUTH.SERVER"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ddok2@bitbucket.org/web_apm/webapm_auth.git"
  },
  "keywords": [
    "WebAPM"
  ],
  "author": "NKIA WebAPM TEAM",
  "license": "NKIA License",
  "homepage": "https://bitbucket.org/web_apm/webapm_auth#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "figlet": "^1.2.0",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongoose": "^4.9.0",
    "morgan": "^1.8.1",
    "pm2": "^2.4.5",
    "react": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.67.2",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-env": "^1.2.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-app": "^2.2.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "del": "^2.2.2",
    "docdash": "^0.4.0",
    "errorhandler": "^1.5.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-exec": "^2.1.3",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-replace": "^0.5.4",
